package com.klayx.asset.model;

import java.math.BigDecimal;
import java.util.Date;

public class AssetGetRequestVO {

	private BigDecimal _id;
	private String isin;
	private BigDecimal _currencyAppId;
	private String currencyConfigName;
	private BigDecimal _custodianAppId;
	private String custodianConfigName;
	private String fromCurrencyConfigName;
	private BigDecimal _fromCurrencyAppId;
	private String toCurrencyConfigName;
	private BigDecimal _toCurrencyAppId;
	private Date referenceDate;
	private String brokerAppConfigName;
	private PreferredAssetGroupVO preferredAssetGroupVO;

	public String getBrokerAppConfigName() {
		return brokerAppConfigName;
	}
	public void setBrokerAppConfigName(String brokerAppConfigName) {
		this.brokerAppConfigName = brokerAppConfigName;
	}
	public BigDecimal get_id() {
		return _id;
	}
	public void set_id(BigDecimal _id) {
		this._id = _id;
	}
	public String getIsin() {
		return isin;
	}
	public void setIsin(String isin) {
		this.isin = isin;
	}
	public BigDecimal get_currencyAppId() {
		return _currencyAppId;
	}
	public void set_currencyAppId(BigDecimal _currencyAppId) {
		this._currencyAppId = _currencyAppId;
	}
	public String getCurrencyConfigName() {
		return currencyConfigName;
	}
	public void setCurrencyConfigName(String currencyConfigName) {
		this.currencyConfigName = currencyConfigName;
	}
	public BigDecimal get_custodianAppId() {
		return _custodianAppId;
	}
	public void set_custodianAppId(BigDecimal _custodianAppId) {
		this._custodianAppId = _custodianAppId;
	}
	public String getCustodianConfigName() {
		return custodianConfigName;
	}
	public void setCustodianConfigName(String custodianConfigName) {
		this.custodianConfigName = custodianConfigName;
	}
	public String getFromCurrencyConfigName() {
		return fromCurrencyConfigName;
	}
	public void setFromCurrencyConfigName(String fromCurrencyConfigName) {
		this.fromCurrencyConfigName = fromCurrencyConfigName;
	}
	public BigDecimal get_fromCurrencyAppId() {
		return _fromCurrencyAppId;
	}
	public void set_fromCurrencyAppId(BigDecimal _fromCurrencyAppId) {
		this._fromCurrencyAppId = _fromCurrencyAppId;
	}
	public String getToCurrencyConfigName() {
		return toCurrencyConfigName;
	}
	public void setToCurrencyConfigName(String toCurrencyConfigName) {
		this.toCurrencyConfigName = toCurrencyConfigName;
	}
	public BigDecimal get_toCurrencyAppId() {
		return _toCurrencyAppId;
	}
	public void set_toCurrencyAppId(BigDecimal _toCurrencyAppId) {
		this._toCurrencyAppId = _toCurrencyAppId;
	}
	public Date getReferenceDate() {
		return referenceDate;
	}
	public void setReferenceDate(Date referenceDate) {
		this.referenceDate = referenceDate;
	}
	public PreferredAssetGroupVO getPagroupvo() {
		return preferredAssetGroupVO;
	}

	public void setPagroupvo(PreferredAssetGroupVO pagroupvo) {
		this.preferredAssetGroupVO = pagroupvo;
	}

	@Override
	public String toString() {
		return "AssetGetRequestVO [_id=" + _id + ", isin=" + isin + ", _currencyAppId=" + _currencyAppId
				+ ", currencyConfigName=" + currencyConfigName + ", _custodianAppId=" + _custodianAppId
				+ ", custodianConfigName=" + custodianConfigName + ", fromCurrencyConfigName=" + fromCurrencyConfigName
				+ ", _fromCurrencyAppId=" + _fromCurrencyAppId + ", toCurrencyConfigName=" + toCurrencyConfigName
				+ ", _toCurrencyAppId=" + _toCurrencyAppId + ", referenceDate=" + referenceDate + "]";
	}
	

}
