package com.klayx.asset.model;

import java.math.BigDecimal;
import java.util.Date;
import java.util.List; 

public class AssetRequestVO {
	private List<AssetAddRequestVO> assetAddRequest;
	private List<AssetGetRequestVO> assetGetRequest;
	private Date referenceDate;
	private String tenantCode;

	private SearchRequestVO searchRequest;
	private SortRequestVO sortRequest;
	
	private int pageNumber;
	private String languageCode;
	private int paginationOffSet;
	private int isDownload;
	private String username;
	private String accessToken;
	private BigDecimal _tenantId;

	public List<AssetAddRequestVO> getAssetAddRequest() {
		return assetAddRequest;
	}

	public void setAssetAddRequest(List<AssetAddRequestVO> assetAddRequest) {
		this.assetAddRequest = assetAddRequest;
	}

	public List<AssetGetRequestVO> getAssetGetRequest() {
		return assetGetRequest;
	}

	public void setAssetGetRequest(List<AssetGetRequestVO> assetGetRequest) {
		this.assetGetRequest = assetGetRequest;
	}

	public String getTenantCode() {
		return tenantCode;
	}

	public void setTenantCode(String tenantCode) {
		this.tenantCode = tenantCode;
	}

	public BigDecimal get_tenantId() {
		return _tenantId;
	}

	public Date getReferenceDate() {
		return referenceDate;
	}

	public void setReferenceDate(Date referenceDate) {
		this.referenceDate = referenceDate;
	}

	public void set_tenantId(BigDecimal _tenantId) {
		this._tenantId = _tenantId;
	}

	public SearchRequestVO getSearchRequest() {
		return searchRequest;
	}

	public void setSearchRequest(SearchRequestVO searchRequest) {
		this.searchRequest = searchRequest;
	}

	public int getPageNumber() {
		return pageNumber;
	}

	public void setPageNumber(int pageNumber) {
		this.pageNumber = pageNumber;
	}

	public String getLanguageCode() {
		return languageCode;
	}

	public void setLanguageCode(String languageCode) {
		this.languageCode = languageCode;
	}

	public int getPaginationOffSet() {
		return paginationOffSet;
	}

	public void setPaginationOffSet(int paginationOffSet) {
		this.paginationOffSet = paginationOffSet;
	}

	public int getIsDownload() {
		return isDownload;
	}

	public void setIsDownload(int isDownload) {
		this.isDownload = isDownload;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}
	
	public String getAccessToken() {
		return accessToken;
	}

	public void setAccessToken(String accessToken) {
		this.accessToken = accessToken;
	}

	public SortRequestVO getSortRequest() {
		return sortRequest;
	}

	public void setSortRequest(SortRequestVO sortRequest) {
		this.sortRequest = sortRequest;
	}

	@Override
	public String toString() {
		return "AssetRequestVO [assetAddRequest=" + assetAddRequest + ", assetGetRequest=" + assetGetRequest
				+ ", referenceDate=" + referenceDate + ", tenantCode=" + tenantCode + ", searchRequest=" + searchRequest
				+ ", sortRequest=" + sortRequest + ", pageNumber=" + pageNumber + ", languageCode=" + languageCode
				+ ", paginationOffSet=" + paginationOffSet + ", isDownload=" + isDownload + ", username=" + username
				+ ", accessToken=" + accessToken + ", _tenantId=" + _tenantId + "]";
	}
	
	
}